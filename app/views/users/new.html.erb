<%= form_for @user do |f| %>
  <table>
    <tr>
      <td>Festival nickname:</td>do
      <td><%= f.text_field :user_name %></td>
    </tr>
    <tr>
      <td>Password:</td>
      <td><%= f.password_field :password %></td>
    </tr>
    <tr>
      <td>Email:</td>
      <td><%= f.text_field :email %></td>
    </tr>
  </table>
  <%= f.submit %>
<% end %>